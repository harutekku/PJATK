<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.121"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="GUI11.MainController">
   <AnchorPane layoutX="-20.0"
               layoutY="-34.0"
               AnchorPane.leftAnchor="0.0"
               AnchorPane.rightAnchor="0.0"
               AnchorPane.topAnchor="0.0">
      <children>
         <HBox maxHeight="1.7976931348623157E308"
               maxWidth="1.7976931348623157E308"
               spacing="10.0"
               AnchorPane.leftAnchor="10.0"
               AnchorPane.rightAnchor="10.0"
               AnchorPane.topAnchor="10.0">
            <children>
               <TextField fx:id="nickField"
                          maxHeight="1.7976931348623157E308"
                          maxWidth="1.7976931348623157E308"
                          minHeight="50.0"
                          minWidth="-Infinity"
                          HBox.hgrow="ALWAYS" />
               <Button maxHeight="1.7976931348623157E308"
                       maxWidth="1.7976931348623157E308"
                       minHeight="-Infinity"
                       minWidth="-Infinity"
                       mnemonicParsing="false"
                       onAction="#addNick"
                       text="Wygeneruj haslo"
                       HBox.hgrow="ALWAYS" />
            </children>
         </HBox>
      </children>
   </AnchorPane>
    <ScrollPane AnchorPane.bottomAnchor="0.0"
                AnchorPane.leftAnchor="0.0"
                AnchorPane.rightAnchor="0.0"
                AnchorPane.topAnchor="100.0">
        <VBox fx:id="listOfElements" />
    </ScrollPane>
</AnchorPane>
